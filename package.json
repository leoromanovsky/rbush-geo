{
  "name": "rbush_geo",
  "version": "0.0.1",
  "description": "Geo specific wrapper for https://github.com/mourner/rbush",
  "homepage": "https://github.com/leoromanovsky/rbush-geo",
  "keywords": [
    "spatial",
    "tree",
    "search",
    "rectangle",
    "index",
    "math"
  ],
  "author": "Leo Romanovsky",
  "repository": {
    "type": "git",
    "url": "git@github.com:leoromanovsky/rbush-geo.git"
  },
  "main": "rbush_geo.js",
  "devDependencies": {
    "rbush": "1.3.4",
    "eslint": "^0.13.0",
    "faucet": "0.0.1",
    "istanbul": "~0.3.2",
    "tape": "^3.0.3"
  },
  "scripts": {
    "test": "eslint rbush_geo.js test/test.js && node test/test.js | faucet"
  },
  "eslintConfig": {
    "rules": {
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "camelcase": 2,
      "space-after-function-name": 2,
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "space-after-keywords": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 2,
      "no-empty": 2,
      "no-new": 2,
      "key-spacing": 2,
      "no-multi-spaces": 2,
      "space-in-brackets": 2,
      "quotes": [
        2,
        "single"
      ],
      "new-cap": 0,
      "no-new-func": 0,
      "curly": 0,
      "no-underscore-dangle": 0,
      "no-constant-condition": 0,
      "no-shadow": 0
    },
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "define": false
    }
  }
}
